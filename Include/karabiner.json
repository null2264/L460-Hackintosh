{
    "title": "ThinkPad L460 Alternative Function Key",
    "rules": [
        {
            "description": "control + f12 to launchpad",
            "manipulators": [
                {
                    "from": {
                        "key_code": "f12",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "launchpad"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "manipulators": [
                {
                    "description": "control + f11 to open mission control",
                    "from": {
                        "key_code": "f11",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "mission_control"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "manipulators": [
                {
                    "description": "control + f10 to open alfred",
                    "from": {
                        "key_code": "f10",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "spacebar",
                            "modifiers": [
                                "command"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "control + f6 to display_brightness_increment",
            "manipulators": [
                {
                    "from": {
                        "key_code": "f6",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "display_brightness_increment"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "control + f5 to display_brightness_decrement",
            "manipulators": [
                {
                    "from": {
                        "key_code": "f5",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "display_brightness_decrement"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "control + f3 to volume_increment",
            "manipulators": [
                {
                    "from": {
                        "key_code": "f3",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "volume_increment"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "control + f2 to volume_decrement",
            "manipulators": [
                {
                    "from": {
                        "key_code": "f2",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "volume_decrement"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "control + f1 to mute",
            "manipulators": [
                {
                    "from": {
                        "key_code": "f1",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "mute"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "[Finder] use enter to open",
            "manipulators": [
                {
                    "conditions": [
                        {
                            "bundle_identifiers": [
                                "^com.apple.finder"
                            ],
                            "type": "frontmost_application_if"
                        }
                    ],
                    "from": {
                        "key_code": "return_or_enter",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "o",
                            "modifiers": [
                                "right_command"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
